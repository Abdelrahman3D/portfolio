.footer
  margin-bottom: 30px
  display: flex
  justify-content: center

.footer-list
  display: flex
  margin: 0
  padding: 0
  list-style: none

.footer-item
  padding: 10px
  color: $dark
  font-size: $font-large
  &:hover
    color: $primary

  +dark()
    color: $white
    &:hover
      color: $primary